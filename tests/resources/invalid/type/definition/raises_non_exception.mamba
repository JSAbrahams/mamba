class MyErr1 isa Exception("Something went wrong")
class MyErr2(msg: String) isa Exception(msg)
class MyErr3(msg: String)

def f(x: Int) -> Int raise [MyErr1, MyErr2, MyErr3] =>
    if x < 0 then
        raise MyErr1()
    else
        if x > 10 then
            raise MyErr2("Greater than 10.")
        else
            return x + 2
